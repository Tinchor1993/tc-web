  "name": "thcapp-cms_fe",
  "version": "1.0.0",
  "description": "Thinking Cap App",
  "engines": {
    "npm": ">=4"
  },
  "scripts": {
    "api": "cross-env NODE_ENV=prod nodemon ./server/index --exec babel-node --presets es2015,stage-2",
    "api:dev": "cross-env NODE_ENV=dev nodemon ./server/index --exec babel-node --presets es2015,stage-2",
    "start": "npm-run-all --parallel open:dist",
    "server:dev": "cross-env NODE_ENV=dev nodemon index.js --exec babel-node --presets es2015,stage-2",
    "server:prod": "cross-env NODE_ENV=prod nodemon index.js --exec babel-node --presets es2015,stage-2",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "open:dist:local": "NODE_ENV=dev babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "predeploy": "npm run clean-dist && babel-node tools/build.js",
    "test": "jest",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "prettier": "prettier --single-quote --no-semi --write '{src,api}/**/*.js'",
    "precommit": "npm run prettier"
  },
